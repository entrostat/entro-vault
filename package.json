{
  "name": "entro-vault",
  "version": "1.0.2",
  "description": "A CLI to handle our vault infrastructure",
  "author": "Kerren Ortlepp @Kerren-Entrostat",
  "bin": {
    "entro-vault": "./bin/run"
  },
  "homepage": "https://github.com/entrostat/entro-vault",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "repository": "entrostat/entro-vault",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.7",
    "fs-extra": "^11.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.13",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.10",
    "chai": "^4",
    "entro-version": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.8.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "entro-vault",
    "dirname": "entro-vault",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "release": "entro-version --master-branch-name=main",
    "release:major": "entro-version --master-branch-name=main --standard-version-flags=\"--release-as=major\"",
    "build:tarballs": "oclif pack tarballs"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/entrostat/entro-vault/issues",
  "keywords": [
    "oclif",
    "vault",
    "cli"
  ],
  "types": "dist/index.d.ts"
}
